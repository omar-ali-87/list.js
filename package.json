{
  "name": "list.js",
  "version": "2.3.1",
  "description": "The perfect library for lists. Supports search, sort, filters and flexibility. Built to be invisible and work on existing HTML",
  "keywords": [
    "list",
    "search",
    "sort",
    "table",
    "dom",
    "html",
    "ui"
  ],
  "author": {
    "name": "Jonny StrÃ¶mberg",
    "email": "jonny.stromberg@gmail.com",
    "url": "https://javve.com"
  },
  "homepage": "https://listjs.com",
  "repository": "git://github.com/javve/list.js.git",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/javve/list.js/issues"
  },
  "dependencies": {
    "string-natural-compare": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/string-natural-compare": "^3.0.4",
    "babel-loader": "^8.2.1",
    "jest": "^29.7.0",
    "jquery": "^3.5.1",
    "prettier": "^2.2.0",
    "terser": "^5.44.1",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0"
  },
  "main": "./dist/list.js",
  "module": "./dist/list.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/list.js",
      "require": "./dist/list.min.js"
    }
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "pnpm exec jest",
    "build": "vite build && vite build --mode minified",
    "watch": "vite build --watch",
    "watch-test": "pnpm exec jest --watch",
    "preversion": "pnpm test && pnpm run build && cp dist/list.min.js docs/assets/javascripts/list.min.js && cp dist/list.min.js.map docs/assets/javascripts/list.min.js.map && cp dist/list.js docs/assets/javascripts/list.js && cp dist/list.js.map docs/assets/javascripts/list.js.map && git add dist && git add docs/assets/javascripts",
    "postversion": "git push --follow-tags origin master && cp package.json docs/_data/pkg.json && git add docs/_data/pkg.json && git commit -m \"pkg.json update\""
  },
  "npmName": "list.js",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js",
        "*.js.map"
      ]
    }
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/types/**/*"
    ],
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "globals": {
      "ts-jest": {
        "useESM": false
      }
    }
  }
}
